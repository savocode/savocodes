{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.6",
        "barryvdh/laravel-debugbar": "^2.0",
        "benwilkins/laravel-fcm-notification": "dev-master",
        "gregwar/image": "2.*",
        "guzzlehttp/guzzle": "~5.0|~6.0",
        "kodeine/laravel-meta": "dev-master",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4.0",
        "propaganistas/laravel-phone": "^3.0",
        "ramsey/uuid": "^3.7",
        "rap2hpoutre/laravel-log-viewer": "^0.10.3",
        "tymon/jwt-auth": "0.5.*",
        "yajra/laravel-datatables-oracle": "^7.5",
        "intervention/image": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/Helpers/AutoloadHelpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "5.7"
    }
}
