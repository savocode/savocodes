    "/register": {
      "post": {
        "tags": [
          "User"
        ],
        "summary": "Register as user (normal|driver) in application.",
        "description": "",
        "operationId": "applyForRegistration",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "email",
            "in": "formData",
            "description": "Email to register for.",
            "required": true,
            "type": "string",
            "default": ""
          },
          {
            "name": "password",
            "in": "formData",
            "description": "Password which will be used for log-in.",
            "required": true,
            "type": "string",
            "default": ""
          },
          {
            "name": "first_name",
            "in": "formData",
            "description": "First name for registration.",
            "required": true,
            "type": "string",
            "default": ""
          },
          {
            "name": "last_name",
            "in": "formData",
            "description": "Last name for registration.",
            "required": true,
            "type": "string",
            "default": ""
          },
          {
            "name": "profession_id",
            "in": "formData",
            "description": "Profession id for physician.",
            "required": true,
            "type": "string",
            "default": ""
          },
          {
            "name": "hospital_id",
            "in": "formData",
            "description": "Hospital id for physician.",
            "required": true,
            "type": "string",
            "default": ""
          },
          {
            "name": "phone",
            "in": "formData",
            "description": "Phone field for user.",
            "required": true,
            "type": "string",
            "default": ""
          },
          {
            "name": "device_token",
            "in": "formData",
            "description": "Device token for push notification.",
            "required": false,
            "type": "string",
            "default": ""
          },
          {
            "name": "device_type",
            "in": "formData",
            "description": "Device type for push notification.",
            "required": false,
            "type": "string",
            "default": "",
            "enum": [
              "ios",
              "android"
            ]
          }
        ],
        "responses": {}
      }
    },
